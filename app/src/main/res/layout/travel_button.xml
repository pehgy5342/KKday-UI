<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <Button android:layout_width="match_parent" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:id="@+id/button2"
            app:layout_constraintHorizontal_bias="1.0" style="@style/Widget.AppCompat.Button.Borderless"/>
    <TextView
            android:text="行程表"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/button2" app:layout_constraintStart_toEndOf="@+id/imageView10"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp"/>
    <ImageView android:layout_width="37dp" android:layout_height="33dp"
               android:background="@drawable/flag"
               app:layout_constraintStart_toStartOf="parent"
               android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginTop="12dp"
               app:layout_constraintTop_toTopOf="@+id/button2" android:id="@+id/imageView10"/>

    <View
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@+id/button2" android:id="@+id/view4"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="108dp"
            android:layout_marginStart="108dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button android:layout_width="match_parent" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/view4" android:id="@+id/button3"
            style="@style/Widget.AppCompat.Button.Borderless"/>

    <TextView
            android:text="地圖"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10" app:layout_constraintStart_toStartOf="@+id/textView11"
            android:layout_marginTop="24dp" app:layout_constraintTop_toBottomOf="@+id/view4"/>
    <ImageView android:layout_width="37dp" android:layout_height="33dp"
               android:background="@drawable/map"
               app:layout_constraintStart_toStartOf="@+id/imageView10" android:id="@+id/imageView11"
               android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/view4"/>
    <View
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="108dp"
            android:layout_marginStart="108dp"
            app:layout_constraintTop_toBottomOf="@+id/button3" android:id="@+id/view5"
            app:layout_constraintEnd_toEndOf="parent"
    />
    <Button android:layout_width="match_parent" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/view5" android:id="@+id/button4"
            style="@style/Widget.AppCompat.Button.Borderless"/>

    <TextView
            android:text="費用項目"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12" app:layout_constraintStart_toStartOf="@+id/textView10"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="@+id/button4"/>
    <ImageView android:layout_width="37dp" android:layout_height="33dp"
               android:background="@drawable/money"
               app:layout_constraintStart_toStartOf="@+id/imageView11"
               android:id="@+id/imageView12" android:layout_marginTop="8dp"
               app:layout_constraintTop_toTopOf="@+id/button4"/>
    <View
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:background="@android:color/darker_gray"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="108dp" android:layout_marginStart="108dp" android:id="@+id/view6"
            app:layout_constraintTop_toBottomOf="@+id/button4"
            app:layout_constraintEnd_toEndOf="parent"/>


    <Button android:layout_width="match_parent" android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/view6"
            style="@style/Widget.AppCompat.Button.Borderless" android:id="@+id/button5"/>

    <TextView
            android:text="注意事項"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView13" app:layout_constraintStart_toStartOf="@+id/textView12"
            android:layout_marginTop="20dp" app:layout_constraintTop_toBottomOf="@+id/view6"/>
    <ImageView android:layout_width="37dp" android:layout_height="33dp"
               android:background="@drawable/msg"
               app:layout_constraintStart_toStartOf="@+id/imageView12"
               android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/view6"/>

</androidx.constraintlayout.widget.ConstraintLayout>